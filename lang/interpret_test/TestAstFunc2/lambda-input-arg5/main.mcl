import "fmt"

$fn = func() {
	4
}

$in1 = 42
$in2 = 13

func apply1($a, $b) {
	$a() + $b # call
}

$apply2 = func($a, $b) {
	$a() + $b # call
}

$out1 = apply1($fn, $in1)
$out2 = $apply2($fn, $in2)
$val = $fn()

$s1 = fmt.printf("%d + %d = %d", $in1, $val, $out1)
$s2 = fmt.printf("%d + %d = %d", $in2, $val, $out2)

test $s1 {}
test $s2 {}
